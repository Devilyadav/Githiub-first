x=int(input())
i=0
while i<9:
    if x==18:
        print("that is required ans")
        break
    elif x<18:
        print("x is small, try again")
        i=i+1
        x=int(input())
        if i<9:
            continue
        else:
            print("GAME OVER")
    else:
        print("x is large, try again")
        i=i+1
        x=int(input())
        if i<9:
            continue
        else:
            print("GAME OVER")
